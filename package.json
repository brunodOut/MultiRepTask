{
  "name": "multireptest",
  "version": "0.0.1",
  "description": "Experimental test for multiple cognitive representations (frames) for the same world information",
  "main": "MultiRepTest.js",
  "type": "module",
  "directories": {
    "lib": "lib"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "mjs",
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/test/utils"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "babel-jest": "^27.2.1",
    "lab.js": "^20.2.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^27.2.1"
  },
  "scripts": {
    "test": "jest --config=jest.config.cjs --verbose --watchAll"
  },
  "keywords": [
    "experiment",
    "lab.js",
    "move.lab",
    "frame",
    "semantics",
    "schemata",
    "cognitive",
    "science"
  ],
  "author": "Bruno Moreira Guedes <moreira.guedes@estudante.uffs.edu.br>",
  "license": "AGPL-3.0"
}
